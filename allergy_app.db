import sqlite3
from werkzeug.security import generate_password_hash

conn = sqlite3.connect("allergy_app.db")
cursor = conn.cursor()

admins = [
    ("admin1", generate_password_hash("password1")),
    ("admin2", generate_password_hash("password2")),
    ("admin3", generate_password_hash("password3")),
]

cursor.executemany("INSERT INTO admins (username, password) VALUES (?, ?)", admins)

conn.commit()
conn.close()
